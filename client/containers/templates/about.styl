@import 'rupture'
@import 'nib'
@import 'jeet'
@import '../../styles/vars.styl'
@import '../../styles/grid.styl'




#about
  height: 3000px
  padding-bottom: 3.5vh
  padding-top: 8.5vmin
  div, .column
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    img
      width: 25vmin
      height: 25vmin
      +below($phablet)
        width: 35vmin
        height: 35vmin
    h1
      width: 100%
    h2
      width: 100%
      padding-bottom: 2%
    i
      color: $white
