.header
  width 100%
  margin 0 auto
  background-color #ffffff



h1
  line-height 1
  text-align center
  font-family 'Montserrat', sans-serif
  text-transform uppercase
  padding 15px

p
  line-height 1.4
  text-align center
  font-family 'Domine', serif


li
  display inline
  padding 15px 25px 15px 25px
  margin 0 auto
  text-align center
  text-transform uppercase
  font-family 'Montserrat', sans-serif
  text-decoration none

a
  text-decoration none
  color green
a:hover
  color purple
.btn:target
  color yellow

